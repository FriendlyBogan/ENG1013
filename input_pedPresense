pin = 0
import time 
from pymata4 import pymata4
board = pymata4.Pymata4()
board.set_pin_mode_analog_input(pin)
try:
    while True:
        value = board.analog_read(pin)
        if value[0] == 0:
            pedPresense = True
except KeyboardInterrupt:
        board.shutdown()
        